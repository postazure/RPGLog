<% if current_user %>
  Welcome <%= current_user.email %>
  <div class="menu">
    <%= link_to raw("Sign Out <i class='unlock alternate icon'></i>"), destroy_user_session_path, class:"item", method: :delete %>
  </div>
<% else %>

  <i class="lock icon"></i> Welcome
  <div class="menu">
    <%= link_to "Sign Up", new_user_registration_path, class:"item" %>
    <%= link_to "Sign In", new_user_session_path, class:"item" %>
  </div>

<% end %>
